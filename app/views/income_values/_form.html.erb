<%= f.date_field :start_time %>
<% unless @incomes.present? %>
  <%= link_to '項目を設定', incomes_path(current_user) %>
<% else %>
  <%= f.collection_select(:income_id, incomes.all, :id, :name, prompt: "選択してください") %>
<% end %>
内容<%= f.text_field :content %>
金額<%= f.text_field :value %>
備考<%= f.text_field :description %>
<%= f.submit %>